<Application x:Class="UITest.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:converter="clr-namespace:UITest.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/TabControlStyle.xaml"/>
                <ResourceDictionary Source="Style/OutLookExpanderStyle.xaml"/>
                
                <ResourceDictionary>
                    <converter:BoolToInverseConverter x:Key="BoolToInverseConverter"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
